<div id="servicesWrapper"class="block ml-0 lg:ml-8" data-services={{services}}>
    <span class="text-gray-900 font-semibold">Which Servcies Are You Interested In?</span>
    <br/>
    <span class=" text-sm mb-4 font-normal">(check all that apply)</span>
  <div class="mt-2" id="checkBoxWrapper" data-active="{{services}}">
    <div>
      <label class="inline-flex items-center cursor-pointer mb-6 lg:mb-2 mt-6">
        <input type="checkbox" value="webDevelopment" name="services" class=" w-6 lg:w-4 h-6 lg:h-4 text-primary form-checkbox cursor-pointer {% for service in services %} {% if service === 'Web Development' %} checked {% endif %} {% endfor %} "/>
        <span class="ml-2">Web Development</span>
      </label>
    </div>
    <div>
      <label class="inline-flex items-center cursor-pointer mb-6 lg:mb-2">
        <input type="checkbox" value="mobileDevelopment" name="services" class="w-6 lg:w-4 h-6 lg:h-4 text-primary form-checkbox cursor-pointer" >
        <span class="ml-2">Mobile Development</span>
      </label>
    </div>
    <div>
      <label class="inline-flex items-center cursor-pointer mb-6 lg:mb-2">
        <input type="checkbox" value="Design" name="services" class="w-6 lg:w-4 h-6 lg:h-4 text-primary form-checkbox cursor-pointer">
        <span class="ml-2">Design</span>
      </label>
    </div>
    <div>
      <label class="inline-flex items-center cursor-pointer mb-6 lg:mb-2">
        <input type="checkbox" value="automation" name="services"  class="w-6 lg:w-4 h-6 lg:h-4 text-primary form-checkbox cursor-pointer">
        <span class="ml-2">Automation</span>
      </label>
    </div>
    <div>
      <label class="inline-flex items-center cursor-pointer mb-6 lg:mb-2">
        <input type="checkbox" value="SEO"  name="services"  class="w-6 lg:w-4 h-6 lg:h-4 text-primary form-checkbox cursor-pointer">
        <span class="ml-2">SEO</span>
      </label>
    </div>
  </div>
</div>